{
  "name": "Rajeev greytHR notification",
  "description": "greytHR login notification",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "16": "./icons/16.png",
    "24": "./icons/24.png",
    "32": "./icons/32.png",
    "48": "./icons/48.png",
    "128": "./icons/128.png"
  },
  "background": {
    "service_worker": "background_1.js"
  },
  "options_page": "./options.html",
  "action": {
    "default_title": "Rajeev greytHR notification",
    "default_icon": {
      "16": "./icons/16.png",
      "24": "./icons/24.png",
      "32": "./icons/32.png",
      "48": "./icons/48.png",
      "128": "./icons/128.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "cookies",
    "storage",
    "tabs",
    "scripting",
    "webNavigation",
    "http://*/",
    "https://*/"
  ],
  "host_permissions": ["<all_urls>"]
}
